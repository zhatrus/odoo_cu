<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_gps_route_trip_import_wizard" model="ir.ui.view">
        <field name="name">gps.route.trip.import.wizard.form</field>
        <field name="model">gps.route.trip.import.wizard</field>
        <field name="arch" type="xml">
            <form string="Import Trips">
                <group>
                    <field name="sheet_id"/>
                    <field name="vehicle_id"/>
                    <field name="date_from"/>
                    <field name="date_to"/>
                </group>
                <footer>
                    <button string="Import" type="object" name="action_import" class="btn-primary"/>
                    <button string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
